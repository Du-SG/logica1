programa {
  funcao inicio() {
  real largura
  real diametro

  escreva ("Me de o diametro do terreno\n")
  leia (diametro)
  escreva ("me de a largura do terreno\n")
  leia (largura)
  escreva (largura * diametro , " Essa é a sua área ")

  }
}


